[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m                button1.Enabled = true;[m
[32m+[m[32m                button1.Text = "Ð¡Ð¾Ð·Ð´Ð°ÑÑ Ð¾ÑÑÑÑ";[m
[32m+[m
[32m+[m[32m                button2.Enabled = true;[m
