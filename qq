[32m+[m[32m        public static void DeletePDF(string pathFolder, params string[] namePdfArr)[m
[32m+[m[32m        {[m
[32m+[m[32m            foreach (var namePdf in namePdfArr)[m
[32m+[m[32m            {[m
[32m+[m[32m                string[] arrayDeleteMini = Directory.GetFiles(pathFolder, namePdf, SearchOption.AllDirectories);[m
[32m+[m[32m                foreach (var item in arrayDeleteMini)[m
[32m+[m[32m                {[m
[32m+[m[32m                    string path = item;[m
[32m+[m[32m                    FileInfo fileInf = new FileInfo(path);[m
[32m+[m[32m                    if (fileInf.Exists)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        fileInf.Delete();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m    #region ÐÑÐ»Ð°Ð´ÐºÐ°[m
