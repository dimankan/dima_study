[32m+[m[32m            //mes();        // ! ÐÑÐ¸Ð±ÐºÐ°: Ð´ÐµÐ»ÐµÐ³Ð°Ñ ÑÐ°Ð²ÐµÐ½ null[m
[32m+[m
[32m+[m[32m            Operation? op = Add;[m
[32m+[m[32m            op -= Add;      // Ð´ÐµÐ»ÐµÐ³Ð°Ñ op Ð¿ÑÑÑ[m
[32m+[m[32m            int n = op(3, 4);       // !ÐÑÐ¸Ð±ÐºÐ°: Ð´ÐµÐ»ÐµÐ³Ð°Ñ ÑÐ°Ð²ÐµÐ½ null[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        delegate void Message();[m
[32m+[m[32m        delegate int Operation(int x, int y);[m
[32m+[m
[32m+[m[32m        #region StaticMethods[m
[32m+[m[32m        static void Hello() => Console.WriteLine("Hello!");[m
[32m+[m[32m        static void HowAreYou() => Console.WriteLine("How are you?");[m
[32m+[m[32m        static int Add(int x, int y) => x + y;[m
[32m+[m[32m        static int Multiply(int x, int y) => x * y;[m
[32m+[m[32m        #endregion[m
[32m+[m
[32m+[m
[32m+[m[32m        #region 1. UseDelegate[m
[32m+[m
[32m+[m[32m        private static void RunUseDelegate()[m
[32m+[m[32m        {[m
[32m+[m[32m            Message mes = Hello;[m
[32m+[m[32m            mes();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 2. Ð Ð°Ð·Ð¼ÐµÑÐµÐ½Ð¸Ðµ Ð² Ð´ÐµÐ»ÐµÐ³Ð°Ñ Ð¼ÐµÑÐ¾Ð´Ð¾Ð² Ñ Ð´ÑÑÐ³Ð¸Ñ ÐºÐ»Ð°ÑÑÐ¾Ð²[m
[32m+[m[32m        private static void RuMethodsFromSeveralClasses()[m
[32m+[m[32m        {[m
[32m+[m[32m            Message message1 = Welcome.Print;[m
[32m+[m[32m            Message message2 = new Hello().Display;[m
[32m+[m
[32m+[m[32m            message1(); // Welcome[m
[32m+[m[32m            message2(); // ÐÑÐ¸Ð²ÐµÑ[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 3. ÐÐ°ÑÐ°Ð¼ÐµÑÑÑ Ð¸ ÑÐµÐ·ÑÐ»ÑÑÐ°Ñ Ð´ÐµÐ»ÐµÐ³Ð°ÑÐ°. Operation[m
[32m+[m[32m        private static void RunUseOperation()[m
[32m+[m[32m        {[m
[32m+[m[32m            Operation operation = Add;      // Ð´ÐµÐ»ÐµÐ³Ð°Ñ ÑÐºÐ°Ð·ÑÐ²Ð°ÐµÑ Ð½Ð° Ð¼ÐµÑÐ¾Ð´ Add[m
[32m+[m[32m            //Operation operation = new Operation(Add); // ÐÐ¸Ð±Ð¾ ÑÐ°Ðº[m
[32m+[m[32m            int result = operation(4, 5);   // ÑÐ°ÐºÑÐ¸ÑÐµÑÐºÐ¸ Add(4, 5)[m
[32m+[m[32m            Console.WriteLine(result);      // 9[m
[32m+[m
[32m+[m[32m            operation = Multiply;           // ÑÐµÐ¿ÐµÑÑ Ð´ÐµÐ»ÐµÐ³Ð°Ñ ÑÐºÐ°Ð·ÑÐ²Ð°ÐµÑ Ð½Ð° Ð¼ÐµÑÐ¾Ð´ Multiply[m
[32m+[m[32m            //Operation operation = new Operation(Multiply); // ÐÐ¸Ð±Ð¾ ÑÐ°Ðº[m
[32m+[m[32m            result = operation(4, 5);       // ÑÐ°ÐºÑÐ¸ÑÐµÑÐºÐ¸ Multiply(4, 5)[m
[32m+[m[32m            Console.WriteLine(result);      // 20[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 4.1 ÐÐ¾Ð±Ð°Ð²Ð»ÐµÐ½Ð¸Ðµ Ð¼ÐµÑÐ¾Ð´Ð¾Ð² Ð² Ð´ÐµÐ»ÐµÐ³Ð°Ñ[m
[32m+[m[32m        private static void RunAddSeveralMethods()[m
[32m+[m[32m        {[m
[32m+[m[32m            Message message = Hello;[m
[32m+[m[32m            message += HowAreYou;  // ÑÐµÐ¿ÐµÑÑ message ÑÐºÐ°Ð·ÑÐ²Ð°ÐµÑ Ð½Ð° Ð´Ð²Ð° Ð¼ÐµÑÐ¾Ð´Ð°[m
[32m+[m[32m            message += Hello;[m
[32m+[m[32m            message += Hello;[m
[32m+[m[32m            message += Hello;[m
[32m+[m[32m            message();              // Ð²ÑÐ·ÑÐ²Ð°ÑÑÑÑ Ð¾Ð±Ð° Ð¼ÐµÑÐ¾Ð´Ð° - Hello Ð¸ HowAreYou, ÑÑÐ¾Ð»ÑÐºÐ¾ ÑÐ°Ð·, ÑÐºÐ¾Ð»ÑÐºÐ¾ Ð±ÑÐ»Ð¾ Ð´Ð¾Ð±Ð°Ð²Ð»ÐµÐ½Ð¾[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 4.2 Ð£Ð´Ð°Ð»ÐµÐ½Ð¸Ðµ Ð¼ÐµÑÐ¾Ð´Ð¾Ð² Ñ Ð´ÐµÐ»ÐµÐ³Ð°ÑÐ°[m
[32m+[m
[32m+[m[32m        private static void RunDeleteSeveralMethods()[m
[32m+[m[32m        {[m
[32m+[m[32m            Message? message = Hello; // ÐÑÐ¿Ð¾Ð»ÑÐ·ÑÐµÐ¼ nullable[m
[32m+[m[32m            message += HowAreYou;[m
[32m+[m[32m            message();  // Ð²ÑÐ·ÑÐ²Ð°ÑÑÑÑ Ð²ÑÐµ Ð¼ÐµÑÐ¾Ð´Ñ Ð¸Ð· message[m
[32m+[m[32m            message -= HowAreYou;   // ÑÐ´Ð°Ð»ÑÐµÐ¼ Ð¼ÐµÑÐ¾Ð´ HowAreYou[m
[32m+[m[32m            if (message != null) message(); // Ð²ÑÐ·ÑÐ²Ð°ÐµÑÑÑ Ð¼ÐµÑÐ¾Ð´ Hello[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 5. ÐÐ±ÑÐµÐ´Ð¸Ð½ÐµÐ½Ð¸Ðµ Ð´ÐµÐ»ÐµÐ³Ð°ÑÐ¾Ð²[m
[32m+[m
[32m+[m[32m        private static void RunUnionDelegate()[m
[32m+[m[32m        {[m
[32m+[m[32m            Message mes1 = Hello;[m
[32m+[m[32m            Message mes2 = HowAreYou;[m
[32m+[m[32m            Message mes3 = mes1 + mes2; // Ð¾Ð±ÑÐµÐ´Ð¸Ð½ÑÐµÐ¼ Ð´ÐµÐ»ÐµÐ³Ð°ÑÑ[m
[32m+[m[32m            mes3(); // Ð²ÑÐ·ÑÐ²Ð°ÑÑÑÑ Ð²ÑÐµ Ð¼ÐµÑÐ¾Ð´Ñ Ð¸Ð· mes1 Ð¸ mes2[m
[32m+[m[32m        }[m
[32m+[m[32m        #endregion[m
[32m+[m[32m        #region 6.1 ÐÑÐ·Ð¾Ð² Ð´ÐµÐ»ÐµÐ³Ð°ÑÐ°. Invoke()[m
[32m+[m[32m        private static void RunInvoke()[m
[32m+[m[32m        {[m
[32m+[m[32m            //// Ð Ð°Ð½ÐµÐµ Ð¿Ð¸ÑÐ°Ð»Ð¸[m
[32m+[m[32m            //Message mes = Hello;[m
[32m+[m[32m            //mes();[m
