[32m+[m
[32m+[m[32m                    if (flag == false)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        return false;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    listPdfEnd.Add(secondFolder + @"\mini report.pdf");[m
[32m+[m[32m                }[m
