[32m+[m[32mï»¿using System.Net;[m
[32m+[m[32musing System;[m
[32m+[m[32musing hap = HtmlAgilityPack;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m
[32m+[m[32mnamespace HttpButtonClick[m
[32m+[m[32m{[m
[32m+[m[32m    public class ButtonClicker[m
[32m+[m[32m    {[m
[32m+[m[32m        public string ButtonClick(string URI, string ButtonValue)[m
[32m+[m[32m        {[m
[32m+[m
[32m+[m
[32m+[m[32m            Uri uri = new Uri(URI);[m
[32m+[m
[32m+[m[32m            using (WebClient wc = new WebClient())[m
[32m+[m[32m            {[m
[32m+[m
[32m+[m[32m                //ÐÐ°Ð³ÑÑÐ¶Ð°ÐµÐ¼ ÑÑÑÐ°Ð½Ð¸ÑÑ HTML Ð² string[m
[32m+[m[32m                string HtmlString = wc.DownloadString(uri);[m
[32m+[m
[32m+[m
[32m+[m[32m                //Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ HtmlAgilityPack[m
[32m+[m[32m                hap.HtmlDocument HtmlDoc = new hap.HtmlDocument();[m
[32m+[m
[32m+[m[32m                //ÐÐ°Ð³ÑÑÐ¶Ð°ÐµÐ¼ Html Ð´Ð¾ÐºÑÐ¼ÐµÐ½Ñ Ð¸Ð· ÑÑÑÐ¾ÐºÐ¸[m
[32m+[m[32m                HtmlDoc.LoadHtml(HtmlString);[m
[32m+[m
[32m+[m[32m                //ÐÑÐ±Ð¸ÑÐ°ÐµÐ¼ Ð²ÑÐµ ÑÐ¾ÑÐ¼Ñ Ð½Ð° ÑÑÑÐ°Ð½Ð¸ÑÐµ[m
[32m+[m[32m                hap.HtmlNodeCollection formNodes = HtmlDoc.DocumentNode.SelectNodes("//form");[m
[32m+[m
[32m+[m[32m                List<HtmlFormValue> HtmlForms = new List<HtmlFormValue>();[m
[32m+[m[32m                var AllInputs = new List<Inputitem>();[m
[32m+[m[32m                if (formNodes != null)[m
[32m+[m[32m                {[m
[32m+[m[32m                    foreach (var formAtribute in formNodes)[m
[32m+[m[32m                    {[m
[32m+[m
[32m+[m[32m                        HtmlFormValue FormAtribute = new HtmlFormValue[m
[32m+[m[32m                        {[m
[32m+[m[32m                            ActionForm = formAtribute.Attributes["action"].Value,[m
[32m+[m[32m                            MethodForm = formAtribute.Attributes["method"].Value,[m
[32m+[m[32m                            NameForm = formAtribute.Attributes["name"].Value[m
[32m+[m[32m                        };[m
[32m+[m
[32m+[m[32m                        //ÐÑÐ±Ð¸ÑÐ°ÐµÐ¼ Ð²ÑÐµ imputs Ð½Ð° ÑÐ¾ÑÐ¼Ðµ[m
[32m+[m[32m                        hap.HtmlNodeCollection inputsNodes = formAtribute?.SelectNodes("//input");[m
[32m+[m
[32m+[m[32m                        if (inputsNodes != null)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            AllInputs = inputsNodes[m
[32m+[m[32m                                .Select(x => new Inputitem[m
[32m+[m[32m                                {[m
[32m+[m[32m                                    InputName = x.Attributes["name"]?.Value ?? "",[m
[32m+[m[32m                                    InputType = x.Attributes["type"].Value,[m
[32m+[m[32m                                    InputValue = x.Attributes["value"]?.Value ?? "",[m
[32m+[m[32m                                    FormAtribute = FormAtribute[m
[32m+[m[32m                                }[m
[32m+[m[32m                         ).ToList();[m
[32m+[m[32m                        }[m
[32m+[m[32m                        FormAtribute.ListInputs = AllInputs;[m
[32m+[m[32m                        HtmlForms.Add(FormAtribute);[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m
[32m+[m[32m                    HtmlFormValue FindForm = AllInputs.First(x => x.InputName == ButtonValue).FormAtribute;[m
[32m+[m
[32m+[m
[32m+[m[32m                    if (FindForm != null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        //Ð¡Ð¾Ð±Ð¸ÑÐ°ÐµÐ¼ Ð·Ð°Ð¿ÑÐ¾Ñ[m
[32m+[m[32m                        //Ð£ÐºÐ°Ð·ÑÐ²Ð°ÐµÐ¼ ÑÐ¿Ð¾ÑÐ¾Ð± ÐºÐ¾Ð´Ð¸ÑÐ¾Ð²ÐºÐ¸[m
[32m+[m[32m                        wc.Headers[HttpRequestHeader.ContentType] = "application/x-www-form-urlencoded";[m
[32m+[m[32m                        //Ð´Ð¾Ð±Ð°Ð²Ð»ÑÐµÐ¼ Ð¿Ð°ÑÐ°Ð¼ÐµÑÑÑ[m
[32m+[m[32m                        string myParameters = string.Join("&", FindForm.GetInputsNameValue());[m
[32m+[m[32m                        //Ð¤Ð¾ÑÐ¼Ð¸ÑÑÐµÐ¼ ÑÑÑÐ¾ÐºÑ Ð½Ð° ÑÐºÐ°Ð·Ð°Ð½Ð½ÑÐ¹ ÑÐµÑÑÑÑ[m
[32m+[m[32m                        string uripath = $"{uri.AbsoluteUri}{FindForm.ActionForm}";[m
[32m+[m[32m                        //Ð¾ÑÐ¿ÑÐ°Ð²Ð»ÑÐµÐ¼ Ð¸ Ð¿Ð¾Ð»ÑÑÐ°ÐµÐ¼ ÑÐµÐ·ÑÐ»ÑÑÐ°Ñ[m
[32m+[m[32m                        string HtmlResult = wc.UploadString(uripath, myParameters);[m
[32m+[m[32m                        return HtmlResult;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else { return ""; }[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m
[32m+[m[32m                    return "";[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
