[32m+[m[32m        /// <param name="disposing">Ð¸ÑÑÐ¸Ð½Ð½Ð¾, ÐµÑÐ»Ð¸ ÑÐ¿ÑÐ°Ð²Ð»ÑÐµÐ¼ÑÐ¹ ÑÐµÑÑÑÑ Ð´Ð¾Ð»Ð¶ÐµÐ½ Ð±ÑÑÑ ÑÐ´Ð°Ð»ÐµÐ½; Ð¸Ð½Ð°ÑÐµ Ð»Ð¾Ð¶Ð½Ð¾.</param>[m
[32m+[m[32m        protected override void Dispose(bool disposing)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (disposing && (components != null))[m
[32m+[m[32m            {[m
[32m+[m[32m                components.Dispose();[m
[32m+[m[32m            }[m
[32m+[m[32m            base.Dispose(disposing);[m
[32m+[m[32m        }[m
