[32m+[m[32m      name=""AdmNar.O_AdmNar_VK""[m
[32m+[m[41m    [m	[32maccess=""allow""/>[m
[32m+[m[32m  </Permissions>-->[m
[32m+[m[32m  <Query name=""{nameTemp}""[m
[32m+[m[41m  [m			[32m defaultSelect=""select0"">[m
[32m+[m[32m    <!--ÐÐ¿ÑÐ¸Ð°Ð½Ð°Ð»ÑÐ½Ñ Parameters-->[m
[32m+[m[32m    <!--<Parameters>[m
[32m+[m[32m      <Parameter name=""code_no"" type=""string""/>[m
