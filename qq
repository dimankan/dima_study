[32m+[m[32m            var row = dt.NewRow();[m
[32m+[m
[32m+[m[32m            foreach (var item in dict)[m
[32m+[m[32m            {[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    row[$"{item.Key}"] = item.Value;[m
[32m+[m[32m                }[m
