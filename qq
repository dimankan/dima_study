[32m+[m[32m        private static string FormationColumns(string nameTemp, List<string> listAlias, List<string> listFields, List<string> listType)[m
[32m+[m[32m        {[m
[32m+[m
[32m+[m[32m            #region ÐÐ²ÐµÑÑÐ½ÑÑ ÑÐ°ÑÑÑ ÑÐ°Ð±Ð»Ð¾Ð½Ð°[m
[32m+[m[32m            string upText = $@"<?xml version=""1.0"" encoding=""utf-8"" ?>[m
[32m+[m[32m<QueryTemplateContainer xmlns=""http://CommonComponents/Templates/QueryTemplate.xsd""[m
[32m+[m[32m                        name=""{nameTemp}""[m
[32m+[m[32m                        version=""""[m
