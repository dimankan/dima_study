[32m+[m[32m        private DataTable GetTransposedTable(DataTable dt)[m
[32m+[m[32m        {[m
[32m+[m[32m            DataTable newTable = new DataTable();[m
[32m+[m[32m            newTable.Columns.Add(new DataColumn("0", typeof(string)));[m
[32m+[m[32m            for (int i = 0; i < dt.Columns.Count; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                DataRow newRow = newTable.NewRow();[m
[32m+[m[32m                newRow[0] = dt.Columns[i].ColumnName;[m
[32m+[m[32m                for (int j = 1; j <= dt.Rows.Count; j++)[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (newTable.Columns.Count < dt.Rows.Count + 1)[m
[32m+[m[32m                        newTable.Columns.Add(new DataColumn(j.ToString(), typeof(string)));[m
[32m+[m[32m                    newRow[j] = dt.Rows[j - 1][i];[m
[32m+[m[32m                }[m
[32m+[m[32m                newTable.Rows.Add(newRow);[m
[32m+[m[32m            }[m
[32m+[m[32m            return newTable;[m
[32m+[m[32m        }[m
