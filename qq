[32m+[m[32m    public class CreateElements[m
[32m+[m[32m    {[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// Ð¡Ð¾Ð·Ð´Ð°Ð½Ð¸Ðµ TextBox Ð´Ð»Ñ ÐÐ»Ð¸Ð°ÑÐ¾Ð².[m[41m [m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="arrayAlias"></param>[m
[32m+[m[32m        /// <param name="f"></param>[m
[32m+[m[32m        /// <returns>ÐÐ¸ÑÑ Ñ Ð¾Ð±ÑÐµÐºÑÐ°Ð¼Ð¸</returns>[m
[32m+[m[32m        public static List<object> Alias(string[] arrayAlias, Form f)[m
[32m+[m[32m        {[m
[32m+[m[32m            List<object> result = new List<object>();[m
[32m+[m
[32m+[m[32m            for (int i = 0, n = 44; i < arrayAlias.Length; i++, n += 25)[m
[32m+[m[32m            {[m
