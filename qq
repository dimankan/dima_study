[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.ComponentModel;[m
[32m+[m[32musing System.Data;[m
[32m+[m[32musing System.Drawing;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m[32musing System.Windows.Forms;[m
[32m+[m[32musing System.IO;[m
[32m+[m
[32m+[m[32musing iTextSharp;[m
[32m+[m[32musing iTextSharp.text;[m
[32m+[m[32musing iTextSharp.text.pdf;[m
[32m+[m[32musing System.Diagnostics;[m
[32m+[m[32musing System.Text.RegularExpressions;[m
[32m+[m[32musing PdfSharp.Pdf.IO;[m
[32m+[m[32musing PdfSharp.Pdf;[m
[32m+[m
[32m+[m[32mnamespace add_jpg_v1.StaticClases[m
[32m+[m[32m{[m
[32m+[m[32m    static class CreatePDF[m
[32m+[m[32m    {[m
[32m+[m[32m        public static bool MergePDFs(IEnumerable<string> fileNames, string targetPdf)[m
[32m+[m[32m        {[m
[32m+[m[32m            bool merged = true;[m
[32m+[m[32m            using (FileStream stream = new FileStream(targetPdf, FileMode.Create))[m
[32m+[m[32m            {[m
[32m+[m[32m                Document document = new Document();[m
[32m+[m[32m                PdfCopy pdf = new PdfCopy(document, stream);[m
[32m+[m[32m                iTextSharp.text.pdf.PdfReader reader = null;[m
[32m+[m[32m                try[m
[32m+[m[32m                {[m
[32m+[m[32m                    document.Open();[m
[32m+[m[32m                    foreach (string file in fileNames)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        reader = new iTextSharp.text.pdf.PdfReader(file);[m
[32m+[m[32m                        pdf.AddDocument(reader);[m
[32m+[m[32m                        reader.Close();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                catch (Exception)[m
[32m+[m[32m                {[m
[32m+[m[32m                    merged = false;[m
[32m+[m[32m                    if (reader != null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        reader.Close();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                finally[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (document != null)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        document.Close();[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return merged;[m
[32m+[m[32m        }[m
[32m+[m[32m        public static void Numeric(string putFile)[m
[32m+[m[32m        {[m
[32m+[m[32m            byte[] bytes = File.ReadAllBytes(putFile);[m
[32m+[m[32m            iTextSharp.text.Font blackFont = FontFactory.GetFont("Arial", 22, iTextSharp.text.Font.BOLD, BaseColor.BLACK);[m
[32m+[m[32m            using (MemoryStream stream = new MemoryStream())[m
[32m+[m[32m            {[m
[32m+[m[32m                iTextSharp.text.pdf.PdfReader reader = new iTextSharp.text.pdf.PdfReader(bytes);[m
[32m+[m[32m                using (PdfStamper stamper = new PdfStamper(reader, stream))[m
[32m+[m[32m                {[m
[32m+[m[32m                    int pages = reader.NumberOfPages;[m
[32m+[m[32m                    for (int i = 1; i <= pages; i++)[m
[32m+[m[32m                    {[m
[32m+[m
[32m+[m[32m                        ColumnText.ShowTextAligned(stamper.GetUnderContent(i), Element.ALIGN_RIGHT, new Phrase((i + 1).ToString(), blackFont), 590f, 15f, 0);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                bytes = stream.ToArray();[m
[32m+[m[32m            }[m
[32m+[m[32m            File.WriteAllBytes(putFile, bytes);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static bool CreateWeekReport(string pathFolder)[m
[32m+[m[32m        {[m
[32m+[m[32m            List<string> mainList = new List<string>();[m
[32m+[m
[32m+[m[32m            string[] mainFolders = Directory.GetDirectories(pathFolder);[m
[32m+[m[32m            foreach (var item in mainFolders)[m
[32m+[m[32m            {[m
[32m+[m[32m                bool flag = CreateReport(item);[m
[32m+[m
[32m+[m[32m                if (!flag) return false;[m
[32m+[m
[32m+[m[32m                mainList.Add(item + @"\Report.pdf");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            string lastNameFolder = new DirectoryInfo(pathFolder).Name;[m
[32m+[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m
[32m+[m[32m                MergePDFs(mainList, pathFolder + $@"\Weekly Report {lastNameFolder}.pdf");[m
[32m+[m[32m                Numeric(pathFolder + $@"\Weekly Report {lastNameFolder}.pdf");[m
[32m+[m
[32m+[m[32m               // CompressPdf(pathFolder + $@"\Weekly Report {lastNameFolder}.pdf");[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m[32m            catch[m
[32m+[m[32m            {[m
[32m+[m[32m                MergePDFs(mainList, pathFolder + @"\Report Week.pdf");[m
[32m+[m[32m                Numeric(pathFolder + @"\Report Week.pdf");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
