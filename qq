[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                Presenter.OnOperand1Changed(Convert.ToInt32(tbOperand1.Text));[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
