[32m+[m[32m        public static DataTable FillDataTable(Dictionary<string, string> dict)[m
[32m+[m[32m        {[m
[32m+[m[32m            DataTable dt = new DataTable();[m
[32m+[m[32m            List<object> obj = new List<object>();[m
[32m+[m
[32m+[m[32m            foreach (var item in dict)[m
[32m+[m[32m            {[m
[32m+[m[32m                dt.Columns.Add(new DataColumn($"{item.Key}", typeof(string)));[m
[32m+[m[32m                obj.Add(item.Value);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            dt.Rows.Add(obj.ToArray());[m
[32m+[m[32m            dt.AcceptChanges();[m
[32m+[m
[32m+[m[32m            return dt;[m
[32m+[m[32m        }[m
[32m+[m
