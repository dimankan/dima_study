[32m+[m[32m{[m
[32m+[m[32m    public class DemoModel[m
[32m+[m[32m    {[m
[32m+[m[32m        // data[m
[32m+[m[32m        public int Operand1 { get; set; }[m
[32m+[m[32m        public int Operand2 { get; set; }[m
[32m+[m
[32m+[m[32m        public int Result { get; private set; }[m
[32m+[m
[32m+[m[32m        // business logic[m
[32m+[m[32m        public void Sum()[m
[32m+[m[32m        {[m
[32m+[m[32m            Result = Operand1 + Operand2;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
