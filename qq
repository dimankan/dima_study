[32m+[m
[32m+[m[32m        public static bool MiniReport(string pathFolder)[m
[32m+[m[32m        {[m
[32m+[m[32m            string pathFileSave = pathFolder + @"\mini report.pdf";[m
[32m+[m
[32m+[m[32m            string descrText = String.Empty;[m
[32m+[m
[32m+[m[32m            //string[] pathImgArray = { pathImg, pathImg1, pathImg };[m
[32m+[m
[32m+[m[32m            string[] pathImgArray = Directory.GetFiles(pathFolder, "*.jpg");[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m            string[] pathTxt = Directory.GetFiles(pathFolder, "*.txt");[m
[32m+[m
[32m+[m[32m            if (pathTxt.Length == 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                MessageBox.Show($"Ð Ð¿Ð°Ð¿ÐºÐµ\n{pathFolder}\nÐ¾ÑÑÑÑÐ²ÑÐµÑ txt ÑÐ°Ð¹Ð»Ð° Ñ â ÑÑÐ»ÑÐ³Ð¸!", "ÐÑÐ¸Ð±ÐºÐ°: â Ð£ÑÐ»ÑÐ³Ð¸", MessageBoxButtons.OK, MessageBoxIcon.Error);[m
[32m+[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m            using (StreamReader sr = new StreamReader(pathTxt[0]))[m
[32m+[m[32m            {[m
[32m+[m[32m                descrText = sr.ReadToEnd();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m
[32m+[m[32m            FillPDF(pathFileSave, pathImgArray, descrText);[m
[32m+[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void FillPDF(string pathFileSave, string[] pathImgArray, string descrText)[m
[32m+[m[32m        {[m
[32m+[m[32m            //var document = CreateDoc(pathFileSave);[m
[32m+[m
[32m+[m[32m            List<string> listImgWithPlus = new List<string>();[m
[32m+[m[32m            List<string> listImgWithoutPlus = new List<string>();[m
[32m+[m
[32m+[m
[32m+[m[32m            foreach (var item in pathImgArray)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (Path.GetFileName(item).Contains('+'))[m
[32m+[m[32m                {[m
[32m+[m[32m                    listImgWithPlus.Add(item);[m
[32m+[m[32m                }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    listImgWithoutPlus.Add(item);[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m
[32m+[m[32m            using (FileStream stream = new FileStream(pathFileSave, FileMode.Create))[m
[32m+[m[32m            {[m
[32m+[m[32m                Document document = new Document(PageSize.A4, 20f, 20f, 10f, 10f);[m
[32m+[m
[32m+[m[32m                PdfWriter? pWriter = PdfWriter.GetInstance(document, stream);[m
[32m+[m
[32m+[m[32m                document.Open();[m
[32m+[m
[32m+[m
[32m+[m[32m                var table = AddElements.GetTableJPEG(listImgWithoutPlus.ToArray());[m
[32m+[m
[32m+[m[32m                PdfPTable tableWithPlus = null;[m
[32m+[m
[32m+[m[32m                if (listImgWithPlus.Count != 0)[m
[32m+[m[32m                    tableWithPlus = AddElements.GetTableJPEGWithPlus(listImgWithPlus.ToArray());[m
[32m+[m
[32m+[m[32m                var text = AddElements.AddText(descrText);[m
[32m+[m[32m                var text1 = AddElements.AddText("\n");[m
[32m+[m
[32m+[m
[32m+[m[32m                document.Add(text);[m
[32m+[m[32m                document.Add(text1);[m
[32m+[m
[32m+[m[32m                if (listImgWithPlus.Count != 0)[m
[32m+[m[32m                {[m
[32m+[m[32m                    document.Add(tableWithPlus);[m
[32m+[m[32m                }[m
[32m+[m[32m                document.Add(table);[m
[32m+[m
[32m+[m[32m                document.Close();[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
