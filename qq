[32m+[m[32m        private static bool CreateReport(string pathFolder)[m
[32m+[m[32m        {[m
[32m+[m[32m            List<string> mainList = new List<string>();[m
[32m+[m
[32m+[m[32m            string[] mainFolders = Directory.GetDirectories(pathFolder);[m
[32m+[m[32m            foreach (var item in mainFolders)[m
[32m+[m[32m            {[m
[32m+[m[32m                bool flag = CreatMiddleReportCollection(item);[m
[32m+[m
[32m+[m[32m                if (!flag) return false;[m
[32m+[m
[32m+[m[32m                mainList.Add(item + @"\middle report.pdf");[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            CreatePDF.MergePDFs(mainList, pathFolder + @"\Report.pdf");[m
[32m+[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static bool CreatMiddleReportCollection(string pathFolder)[m
[32m+[m[32m        {[m
