[32m+[m
[32m+[m[32m                        button2.Enabled = true;[m
[32m+[m[32m                        button2.BackColor = System.Drawing.Color.FromArgb(((int)(((byte)(201)))), ((int)(((byte)(127)))), ((int)(((byte)(108)))));[m
[32m+[m[32m                        button2.ForeColor = System.Drawing.Color.FromArgb(((int)(((byte)(240)))), ((int)(((byte)(240)))), ((int)(((byte)(236)))));[m
