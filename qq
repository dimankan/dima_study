[32m+[m[32m            Encoding.RegisterProvider(CodePagesEncodingProvider.Instance);[m
[32m+[m
[32m+[m[32m            string ttf = Path.Combine(Environment.GetFolderPath(Environment.SpecialFolder.Fonts), "ARIAL.TTF");[m
[32m+[m
[32m+[m[32m            BaseFont baseFont = BaseFont.CreateFont(ttf, BaseFont.IDENTITY_H, BaseFont.NOT_EMBEDDED);[m
[32m+[m[32m            var font = new iTextSharp.text.Font(baseFont, iTextSharp.text.Font.DEFAULTSIZE, iTextSharp.text.Font.NORMAL);[m
[32m+[m
[32m+[m
[32m+[m[32m            Paragraph result = new Paragraph(str, font);[m
[32m+[m[32m            result.Alignment = Element.ALIGN_CENTER;[m
[32m+[m
[32m+[m[32m            return result;[m
[32m+[m[32m        }[m
