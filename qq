[32m+[m[32mï»¿using MVPDemo.Model;[m
[32m+[m[32musing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace MVPDemo.Presenter[m
[32m+[m[32m{[m
[32m+[m[32m    public class DemoPresenter[m
[32m+[m[32m    {[m
[32m+[m[32m        private DemoModel _model;[m
[32m+[m[32m        private DemoView _view;[m
[32m+[m
[32m+[m[32m        public DemoPresenter(DemoModel model, DemoView view)[m
[32m+[m[32m        {[m
[32m+[m[32m            this._model = model;[m
[32m+[m[32m            this._view = view;[m
[32m+[m
[32m+[m[32m            view.Presenter = this;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public void OnOperand1Changed(int operand1)[m
[32m+[m[32m        {[m
[32m+[m[32m            _model.Operand1 = operand1;[m
[32m+[m[32m        }[m[41m [m
[32m+[m[32m        public void OnOperand2Changed(int operand1)[m
[32m+[m[32m        {[m
[32m+[m[32m            _model.Operand2 = operand1;[m
[32m+[m[32m        }[m
[32m+[m[32m        public void OnSumClicked()[m
[32m+[m[32m        {[m
[32m+[m[32m            _model.Sum();[m
[32m+[m
[32m+[m[32m            UpdateView();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void UpdateView()[m
[32m+[m[32m        {[m
[32m+[m[32m            _view.UpdateView(_model.Operand1, _model.Operand2, _model.Result);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
