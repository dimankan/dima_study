[36m@@ -0,0 +1,73 @@[m
[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m[32musing System.Reflection;[m
[32m+[m
[32m+[m[32mnamespace Study[m
[32m+[m[32m{[m
[32m+[m[32m    internal class Reflect[m
[32m+[m[32m    {[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// ÐÐ¾ÐºÐ°Ð·Ð°ÑÑ ÑÐ»ÐµÐ½Ñ ÐºÐ»Ð°ÑÑÐ°. ÐÑÐ¸Ð²Ð°ÑÐ½ÑÑ Ð½ÐµÑ[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="nameClass"></param>[m
[32m+[m[32m        public static void ShowMemberInfoName(string nameClass)[m
[32m+[m[32m        {[m
[32m+[m[32m            Type type = Type.GetType("Study." + nameClass);[m
[32m+[m
[32m+[m[32m            var members = type.GetMembers();[m
[32m+[m
[32m+[m
[32m+[m[32m            foreach (MemberInfo memberInfo in members)[m
[32m+[m[32m            {[m
[32m+[m[32m                Console.WriteLine(memberInfo.Name);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// ÐÐ¾ÐºÐ°Ð·Ð°ÑÑ Ð¿ÑÐ¸Ð²Ð°ÑÐ½ÑÐµ ÑÐºÐ·ÐµÐ¼Ð¿Ð»ÑÑÐ½ÑÐµ ÑÐ»ÐµÐ½Ñ[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="nameClass"></param>[m
[32m+[m[32m        public static void ShowMemberInfoNameNonPublicORInstance(string nameClass)[m
[32m+[m[32m        {[m
[32m+[m[32m            Type type = Type.GetType("Study." + nameClass);[m
[32m+[m
[32m+[m[32m            var members = type.GetMembers(BindingFlags.Instance | BindingFlags.NonPublic);[m
[32m+[m
[32m+[m
[32m+[m[32m            foreach (MemberInfo memberInfo in members)[m
[32m+[m[32m            {[m
[32m+[m[32m                Console.WriteLine(memberInfo.Name);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        public static void ChangeField()[m
[32m+[m[32m        {[m
[32m+[m[32m            Student student = new Student();[m
[32m+[m
[32m+[m[32m            Type type = student.GetType();[m
[32m+[m
[32m+[m[32m            var fields = type.GetFields(BindingFlags.NonPublic | BindingFlags.Instance);[m
[32m+[m
[32m+[m[32m            foreach (FieldInfo fieldInfo in fields)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (fieldInfo.Name == "_temp")[m
[32m+[m[32m                {[m
[32m+[m[32m                    var value = fieldInfo.GetValue(student);[m
[32m+[m[32m                    Console.WriteLine($"Before: {value}");[m
[32m+[m
[32m+[m[32m                    fieldInfo.SetValue(student, 15);[m
[32m+[m
[32m+[m[32m                    value = fieldInfo.GetValue(student);[m
[32m+[m[32m                    Console.WriteLine($"After: {value}");[m
[32m+[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
