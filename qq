[32m+[m[32m        public static PdfPTable GetTableJPEG(string[] pathImgArray)[m
[32m+[m[32m        {[m
[32m+[m[32m            PdfPTable result = new PdfPTable(2);[m
[32m+[m[32m            result.WidthPercentage = 100;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m            foreach (var item in pathImgArray)[m
[32m+[m[32m            {[m
[32m+[m[32m                var jpg = iTextSharp.text.Image.GetInstance(item);[m
[32m+[m[32m                var image = System.Drawing.Image.FromFile(item);[m
[32m+[m[32m                if (image.Width > image.Height)[m
[32m+[m[32m                {[m
[32m+[m[32m                    jpg.RotationDegrees = 270f;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                result.AddCell(jpg);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (pathImgArray.Length % 2 == 1)[m
[32m+[m[32m            {[m
[32m+[m[32m                result.AddCell(new Phrase(""));[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m[32m            return result;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static PdfPTable GetTableJPEGWithPlus(string[] pathImgArray)[m
[32m+[m[32m        {[m
[32m+[m[32m            PdfPTable result = new PdfPTable(1);[m
[32m+[m[32m            result.WidthPercentage = 100;[m
