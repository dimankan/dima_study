[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m[32musing System.Windows.Forms;[m
[32m+[m
[32m+[m[32mnamespace add_jpg_v1.StaticClases[m
[32m+[m[32m{[m
[32m+[m[32m    static class OpenFolder[m
[32m+[m[32m    {[m
[32m+[m[32m        public static string ChooseFolder(string path)[m
[32m+[m[32m        {[m
[32m+[m[32m            FolderBrowserDialog folderBrowserDialog1 = new FolderBrowserDialog();[m
[32m+[m[32m            folderBrowserDialog1.SelectedPath = path;[m
[32m+[m[32m            if (folderBrowserDialog1.ShowDialog() == DialogResult.OK)[m
[32m+[m[32m            {[m
[32m+[m[32m                path = folderBrowserDialog1.SelectedPath;[m
[32m+[m[32m            }[m
[32m+[m[32m            return path;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
