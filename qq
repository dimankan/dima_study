[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Linq;[m
[32m+[m
[32m+[m[32mnamespace CodeWars[m
[32m+[m[32m{[m
[32m+[m[32m    internal class Program[m
[32m+[m[32m    {[m
[32m+[m[32m        static void Main(string[] args)[m
[32m+[m[32m        {[m
[32m+[m[32m            int n = 531;[m
[32m+[m[32m            decimal result = 1;[m
[32m+[m[32m            for (int i = 1; i <= n; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                result = i * result;[m
[32m+[m[32m            }[m
[32m+[m[32m            string str = result.ToString();[m
[32m+[m[32m            char[] chars = str.ToCharArray();[m
[32m+[m[32m            Array.Reverse(chars);[m
[32m+[m
[32m+[m[32m            var re = chars.TakeWhile(x => x == '0');[m
[32m+[m[32m            Console.WriteLine(re.Count());[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
