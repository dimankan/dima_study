[32m+[m[32m      <!--ÐÐ¿ÑÐ¸Ð°Ð½Ð°Ð»ÑÐ½Ð¾ SqlParameter-->[m
[32m+[m[32m      <!--<SqlParameter name=""code_no"" oraDbType=""varchar2"" type=""string""/>[m
