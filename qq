[32m+[m[32m";[m
[32m+[m[32m            #endregion[m
[32m+[m[32m            #region ÐÐ¸Ð¶Ð½ÑÑ ÑÐ°ÑÑÑ ÑÐ°Ð±Ð»Ð¾Ð½Ð°[m
[32m+[m[32m            string downText = $@"      <!--From Ð¸ Join Ð½ÑÐ¶Ð½Ð¾ Ð´Ð¾Ð´ÐµÐ»Ð°ÑÑ-->[m
[32m+[m[32m      <From>[m
[32m+[m[32m        <!--<Table scheme=""VIO$INF"" name=""rfn_doc"" alias=""d""/>-->[m
[32m+[m[32m      </From>[m
[32m+[m[32m      <Join mode=""LEFT"">[m
[32m+[m[32m        <!--<Table scheme=""VIO$INF"" name=""R$RFN_DOC"" alias=""rd"" />[m
[32m+[m[32m        <On>d.doc_type = rd.id</On>-->[m
[32m+[m[32m      </Join>[m
[32m+[m[32m      <!--ÐÐ¿ÑÐ¸Ð¾Ð½Ð°Ð»ÑÐ½Ð¾-->[m
[32m+[m[32m      <!--<Where>[m
[32m+[m[32m        <Expression>[m
[32m+[m[32m          (dvk.CODE_NO=:code_no OR '9965'=:code_no OR '0000'=:code_no OR '9961'=:code_no OR[m
[32m+[m[32m          dvk.CODE_NO IN  (SELECT KOD[m
[32m+[m[32m          FROM MDM_LOCAL.THES_SOUN SOUN[m
[32m+[m[32m          WHERE SOUN.KODV = :code_no AND[m
[32m+[m[32m          SOUN.IS_CLOSED = 0 AND SOUN.IS_ANNULED = 0 AND SOUN.TRS_CANCEL_ID IS NULL AND SYSDATE BETWEEN DATE_FROM AND DATE_TO[m
[32m+[m[32m          )[m
[32m+[m[32m          )[m
[32m+[m[32m          and (d.doc_date between :datefrom and :dateto or d.doc_date is null)[m
[32m+[m[32m          and (ds.status_doc = 30)[m
[32m+[m[32m        </Expression>[m
[32m+[m[32m      </Where>-->[m
[32m+[m[32m      <!--ÐÐ¿ÑÐ¸Ð¾Ð½Ð°Ð»ÑÐ½Ð¾-->[m
[32m+[m[32m      <!--Order>[m
[32m+[m[32m        <Item>[m
[32m+[m[32m          <Field table=""d""  column=""doc_date"" />[m
[32m+[m[32m        </Item>[m
[32m+[m[32m      </Order>-->[m
[32m+[m[32m    </Select>[m
[32m+[m[32m  </Query>[m
[32m+[m[32m</QueryTemplateContainer>";[m
[32m+[m[32m            #endregion[m
[32m+[m
[32m+[m
[32m+[m[32m            string columns = String.Empty;[m
[32m+[m[32m            for (int i = 0; i < listAlias.Count(); i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                switch (listType[i])[m
[32m+[m[32m                {[m
[32m+[m[32m                    case "string":[m
[32m+[m[32m                    case "decimal":[m
