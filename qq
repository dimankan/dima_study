[32m+[m[32mï»¿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Text.RegularExpressions;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace WinForms_Ð¸Ð³ÑÑ_Ñ_Ð´Ð¸Ð·Ð°Ð¹Ð½ÐµÑÐ¾Ð¼[m
[32m+[m[32m{[m
[32m+[m[32m    /// <summary>[m
[32m+[m[32m    /// ÐÐ¾Ð»ÑÑÐµÐ½Ð¸Ðµ Ð¼Ð°ÑÑÐ¸Ð²Ð° ÑÐ»ÐµÐ¼ÐµÐ½ÑÐ¾Ð² (ÐÐ»Ð¸Ð°Ñ Ð¸Ð»Ð¸ Ð¿Ð¾Ð»Ðµ)[m
[32m+[m[32m    /// </summary>[m
[32m+[m[32m    public static class Separated[m
[32m+[m[32m    {[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// Ð¡Ð¾Ð·Ð´Ð°ÐµÐ¼ Ð¼Ð°ÑÑÐ¸Ð² ÑÐ»Ð¾Ð², Ð² ÐºÐ¾ÑÐ¾ÑÑÑ ÐµÑÑÑ ÑÐ¾ÑÐºÐ°.[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="str">ÐÐµÑÐ²Ð¸ÑÐ½ÑÐ¹ Ð·Ð°Ð¿ÑÐ¾Ñ</param>[m
[32m+[m[32m        /// <returns></returns>[m
[32m+[m[32m        private static string[] FiltredTrash(string query)[m
[32m+[m[32m        {[m
[32m+[m
[32m+[m[32m            string[] mas1 = query.Split(' ', ',', '\r', '\n');[m
[32m+[m[32m            List<string> list = new List<string>();[m
[32m+[m
[32m+[m[32m            foreach (var item in mas1)[m
[32m+[m[32m            {[m
[32m+[m[32m                Regex regex = new Regex("[.]");[m
[32m+[m[32m                bool match = regex.IsMatch(item);[m
[32m+[m[32m                if (match)[m
[32m+[m[32m                    list.Add(item.ToLower());[m
[32m+[m[32m            }[m
[32m+[m[32m            return list.ToArray();[m
[32m+[m[32m        }[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// ÐÐ¾Ð»ÑÑÐ°ÐµÐ¼ Ñ Ð¾Ð±ÑÐµÐ³Ð¾ Ð·Ð°Ð¿ÑÐ¾ÑÐ° Ð¼Ð°ÑÑÐ¸Ð² Ð°Ð»Ð¸Ð°ÑÐ¾Ð²[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="mas"></param>[m
[32m+[m[32m        /// <returns></returns>[m
[32m+[m[32m        public static string[] GetAlias(string query)[m
[32m+[m[32m        {[m
[32m+[m[32m            string[] firstSepar = FiltredTrash(query);[m
[32m+[m
[32m+[m[32m            string[] result = new string[firstSepar.Length];[m
[32m+[m
[32m+[m[32m            for (int i = 0; i < firstSepar.Length; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                int index = firstSepar[i].IndexOf('.');[m
[32m+[m[32m                result[i] = firstSepar[i].Substring(0, index);[m
[32m+[m[32m            }[m
[32m+[m[32m            return result;[m
[32m+[m[32m        }[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// ÐÐ¾Ð»ÑÑÐ°ÐµÐ¼ Ð¼Ð°ÑÑÐ¸Ð² Ð¿Ð¾Ð»ÐµÐ¹[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="query"></param>[m
[32m+[m[32m        /// <returns></returns>[m
[32m+[m[32m        public static string[] GetFields(string query)[m
[32m+[m[32m        {[m
[32m+[m[32m            string[] firstSepar = FiltredTrash(query);[m
[32m+[m
[32m+[m[32m            string[] result = new string[firstSepar.Length];[m
[32m+[m
[32m+[m[32m            for (int i = 0; i < firstSepar.Length; i++)[m
[32m+[m[32m            {[m
[32m+[m[32m                int index = firstSepar[i].IndexOf('.');[m
[32m+[m[32m                result[i] = firstSepar[i].Substring(index + 1);[m
[32m+[m[32m            }[m
[32m+[m[32m            return result;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
