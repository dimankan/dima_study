[32m+[m[32m                        break;[m
[32m+[m[32m                    case "date":[m
[32m+[m[32m                        columns += $"<Column name=\"{listFields[i]}\" type=\"{listType[i]}\" column=\"{listFields[i]}\" table=\"{listAlias[i]}\" options=\"DataSetDateTime.Unspecified\"/>\n";[m
[32m+[m[32m                        break;[m
[32m+[m[32m                    default:[m
[32m+[m[32m                        break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return $"{upText}\n<Columns>\n{columns}\n</Columns>\n{downText}";[m
[32m+[m[32m        }[m
[32m+[m[32m        /// <summary>[m
[32m+[m[32m        /// ÐÐµÐ½ÐµÑÐ°ÑÐ¸Ñ Ð¸ ÑÐ¾ÑÑÐ°Ð½ÐµÐ½Ð¸Ðµ ÑÐ°Ð±Ð»Ð¾Ð½Ð° Ð¿Ð¾ Ð¾Ð¿ÑÐµÐ´ÐµÐ»ÐµÐ½Ð½Ð¾Ð¼Ñ Ð°Ð´ÑÐµÑÑ[m
[32m+[m[32m        /// </summary>[m
[32m+[m[32m        /// <param name="path"></param>[m
[32m+[m[32m        /// <param name="name"></param>[m
[32m+[m[32m        /// <param name="listAlias"></param>[m
[32m+[m[32m        /// <param name="listFields"></param>[m
[32m+[m[32m        /// <param name="listType"></param>[m
[32m+[m[32m        public static void SaveFiles(string path, string name, List<string> listAlias, List<string> listFields, List<string> listType)[m
[32m+[m[32m        {[m
[32m+[m[32m            string fullName = $"{path}/{name}.txt";[m
[32m+[m
[32m+[m[32m            string text = FormationColumns(name, listAlias, listFields, listType);[m
[32m+[m
[32m+[m[32m            using (FileStream file = new FileStream(fullName, FileMode.Create))[m
[32m+[m[32m            {[m
[32m+[m[32m                using (StreamWriter stream = new StreamWriter(file))[m
[32m+[m[32m                {[m
[32m+[m[32m                    stream.WriteLine(text);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
